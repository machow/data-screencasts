{
    "image": "mcr.microsoft.com/devcontainers/python:3",
    "features": {
      "ghcr.io/rocker-org/devcontainer-features/quarto-cli:1": {
        "version": "1.4.554"
      }
    },
    "forwardPorts": [
      8888
    ],
    "portsAttributes": {
      "8888": {
        "label": "Jupyter"
      }
    },
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-toolsai.jupyter",
          "Posit.shiny-python"
        ]
      }
    },
    "onCreateCommand": {
      "prep-python": "python3 -m pip install -r requirements.txt"
    }
}
